{
    "name": "numpad-highlight",
    "displayName": "Numpad Highlight",
    "description": "Syntax highlighting for the numpad language.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "numpad",
            "aliases": ["Numpad", "numpad"],
            "extensions": [".npd"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "numpad",
            "scopeName": "source.npd",
            "path": "./syntaxes/numpad.tmLanguage.json"
        }]
    }
}